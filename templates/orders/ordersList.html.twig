{% extends "account/account.html.twig" %}

{% block title %}Список замовлень{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('orders') }}
{% endblock %}

{% block profile_data %}
    <div id="dark"></div>
    <div class="wrappers_order">
    <div class="container-context-order">
        {% if product_pager is not empty %}
            <div class="test_order_header">
                <p>Номер Замовлення</p>
                <p>Дата Замовлення</p>
                <p>Статус Замовлення </p>
             </div>
                {% for order in product_pager %}
                <a href="{{ path('app_user_order', { id: order.id }) }}">
                    <div class="container_order">
                        <p class="number_order">№{{ order.id }}</p>
                        {% set mnths = ['','січень','лютий','березень','квітень','травень','червень','липень','серпень','вересень','жовтень','листопад','грудень'] %}
                        <time>{{order.orderDate|date('d ')}}{{mnths[order.orderDate|date('n')]}}{{order.orderDate|date(' Y H:i')}}</time>
                        {% if order.isStatus is null %}
                        <p class="text_status_list" style="color: #83add8;">Очікує підтвердження</p>
                        {% elseif order.isStatus == false %}
                        <p class="text_status_list" style="color: #ff5d52;">Скасовано</p>
                        {% else %}
                        <p class="text_status_list" style="color: #a5c965;">Підтверджено</p>
                        {% endif%}
                </div>
                </a>
                {% endfor %}
        {% else %}
